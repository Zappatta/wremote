{
  "name": "wremote_server",
  "version": "1.0.2",
  "description": "Web interface for Zappatta's Gree A/C RC",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && ./node_modules/webpack/bin/webpack.js && babel src -d build",
    "build-nc": "babel src -d build",
    "start": "NODE_ENV=production node build/main.js",
    "start-dev": "./node_modules/nodemon/bin/nodemon.js src/main.js --exec babel-node src/main.js --presets env",
    "start-debug": "./node_modules/nodemon/bin/nodemon.js %NODE_DEBUG_OPTION% src/main.js --exec babel-node index.js --presets env"
  },
  "keywords": [
    "gree",
    "tadiran",
    "arduino",
    "esp8622"
  ],
  "author": "Yoske Shamam",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.4",
    "google-auth-library": "^2.0.1",
    "moment": "^2.18.1",
    "mqtt": "^2.18.2",
    "nodemon": "^1.18.9",
    "npm": "^6.14.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-filehash": "^6.9.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "node-sass": "^4.9.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue": "^2.4.2",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "vuetify": "^0.15.7",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
